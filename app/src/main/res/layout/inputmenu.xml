<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="39dp"
        android:layout_y="330dp"
        android:text="제품 이름" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="38dp"
        android:layout_y="394dp"
        android:text="제품 가격" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="131dp"
        android:layout_y="318dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="편의점 도시락" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="304dp"
        android:layout_height="wrap_content"
        android:layout_x="129dp"
        android:layout_y="381dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="000원" />

    <ImageButton
        android:id="@+id/inputimg"
        android:layout_width="288dp"
        android:layout_height="259dp"
        android:layout_x="42dp"
        android:layout_y="36dp"
        android:onClick="onClick"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="89dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="145dp"
        android:layout_y="450dp"
        android:text="등록"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="44dp" />

</AbsoluteLayout>